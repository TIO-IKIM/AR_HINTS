{
    "workspaceFolder": "/app",
    "dockerComposeFile": "docker-compose.yml",
    "service": "devcontainer",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/usr/bin/python3",
                "jupyter.jupyterServerType": "local",
                "jupyter.notebookFileRoot": "/app"
            }
        }
    }
}